# StrongEd$Mode = FrontEnd
# This is a description of the wimp interface for stubber

tool_details_start
   name          "Stubber";
   command_is    "<stubber$Dir>.stubber ";
   version       "1.05 (2005-03-13)";
   wimpslot       1400k;
   has_extended_cmdline;
tool_details_end

metaoptions_start
   has_text_window;
   display_dft_is text;
metaoptions_end

fileoutput_start
   output_dft_is produces_output;
fileoutput_end

dbox_start
   #icon mappings
   icons_start
      icn 3 maps_to string;
      
   icons_end
   
   #default icon values
   defaults
      icn 3 "";      
      
   #drags to dbox (and icon bar)
   imports_start
      drag_to icn 3 inserts icn 3;
      drag_to iconbar inserts icn 3;
   imports_end
dbox_end


order_is
 icn 3;

